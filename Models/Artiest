namespace theater_laak.Models;

public class Artiest {
    public int ArtiestId;
    public string Voornaam;
    public string Achternaam;
    public DateOnly? GeboorteDatum;
    public string? Telefoonnummer;
    public string? Email;
    public string? Geslacht; // Man / Vrouw / Anders
    public int? ArtiestenGroepId; // FK naar ArtiestenGroep.ArtiestenGroepId;
}